  "scripts": {
    "start": "./node_modules/.bin/pm2 start processes.json",
    "dev": "./node_modules/.bin/pm2 start processes.dev.json",
    "stop": "./node_modules/.bin/pm2 stop processes.json",
    "smp": "pm2=\"./node_modules/.bin/pm2-runtime\" && rm -rf logs/*.log && NODE_ENV=development TZ=UTC SMP_LANG=ru DEF_PM2_WATCH=1 $pm2 start app.js --error=logs/smp.stderr.log --output=logs/smp.stdout.log --name=\"smp\" --node-args=\"$NODE_DEBUG_OPTION\"",
    "logs": "./node_modules/.bin/pm2 logs",
    "kill": "./node_modules/.bin/pm2 kill",
    "status": "./node_modules/.bin/pm2 status",
    "monit": "./node_modules/.bin/pm2 monit",
    "db": "DB=rvision_ru SMP_LANG=ru TZ=utc node node_modules/.bin/db-migrate up all",
    "migrate": "DB=rvision_ru SMP_LANG=ru TZ=utc node node_modules/.bin/db-migrate",
    "sencha": "cd assets && _JAVA_OPTIONS=\"-Xms1024m -Xmx2048m\" sencha app build development",
    "locale": "cd migrations/data/localization-utils && node checkLocalization.js && node updateDB.js",
    "precommit": "lint-diff",
    "test": "jest",
    "tslint": "tslint -p tsconfig.json --exclude '**/*.d.ts' --fix api/**/*.ts && tslint -p services/internal-scanner/tsconfig.json --fix services/**/*.ts",
    "prettier": "prettier --write api/**/*.ts && prettier --write services/**/*.ts",
    "build": "npm run prettier && npm run tslint && cd services/internal-scanner && ../../node_modules/typescript/bin/tsc"
  },
